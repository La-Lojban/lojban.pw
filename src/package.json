{
  "name": "lojban-made-easy",
  "version": "2.0.0",
  "scripts": {
    "dev": "NODE_ENV=development next",
    "build": "npx next telemetry disable && next build",
    "start": "next start",
    "print": "yarn build && yarn print_service && pkill node",
    "publication": "gh-pages --repo https://github.com/La-Lojban/lojban-made-easy.git -d out -t true",
    "deploy": "npx next telemetry disable && rm -rf node_modules/.cache && rimraf out && next build && yarn print_service && next export && touch out/.nojekyll && yarn publication && pkill node && cp -avr public/assets/* out/assets/",
    "typecheck": "tsc",
    "print_service": "next start & sleep 5 && node lib/printer && ./lib/printer/shrink-pdf.sh"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.7.7",
    "@heroicons/react": "^2.0.13",
    "classnames": "2.3.2",
    "date-fns": "^2.29.3",
    "gray-matter": "4.0.3",
    "jsdom": "^21.1.0",
    "next": "^13.1.4",
    "node-html-parser": "^6.1.4",
    "nprogress": "^0.2.0",
    "puppeteer": "19.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-raw": "^6.1.1",
    "rehype-stringify": "^9.0.3",
    "remark-deflist": "^1.0.0",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "sass": "^1.57.1",
    "tailwindcss-typography": "^3.1.0",
    "typescript": "^4.9.4",
    "unified": "^10.1.2"
  },
  "devDependencies": {
    "@next/env": "^13.1.4",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.18",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.27",
    "autoprefixer": "^10.4.13",
    "babel-plugin-transform-define": "^2.1.0",
    "gh-pages": "^5.0.0",
    "postcss": "^8.4.21",
    "rimraf": "^4.1.1",
    "tailwind-scrollbar": "^2.1.0",
    "tailwindcss": "^3.2.4"
  },
  "license": "MIT"
}
