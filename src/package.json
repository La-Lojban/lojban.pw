{
  "name": "lojban-made-easy",
  "version": "2.0.0",
  "scripts": {
    "dev": "NODE_ENV=development next",
    "start": "NODE_ENV=production next start",
    "compile": "yarn build && pkill node",
    "build": "next telemetry disable && next build && yarn print && yarn export && touch out/.nojekyll && cp -avr /app/service/DNS/* out/ && cp -avr /vreji out/",
    "print": "mkdir -p /vreji/uencu && next start & sleep 5 && node lib/printer && ./lib/printer/shrink-pdf.sh",
    "export": "next export",
    "clean": "rm -rf node_modules && rm -rf .next",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-brands-svg-icons": "^6.3.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.7.12",
    "@heroicons/react": "^2.0.16",
    "classnames": "2.3.2",
    "date-fns": "^2.29.3",
    "gray-matter": "^4.0.3",
    "html-react-parser": "^3.0.9",
    "jsdom": "^21.1.0",
    "next": "^13.2.1",
    "next-pwa": "^5.6.0",
    "node-html-parser": "^6.1.5",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-image-gallery": "^1.2.11",
    "rehype-mathjax": "^4.0.2",
    "rehype-raw": "^6.1.1",
    "rehype-stringify": "^9.0.3",
    "remark": "^14.0.2",
    "remark-deflist": "^1.0.0",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "sass": "^1.58.3",
    "socket.io-client": "^4.6.1",
    "tailwindcss-typography": "^3.1.0",
    "to-vfile": "^7.2.4",
    "typescript": "^4.9.5",
    "unified": "^10.1.2"
  },
  "devDependencies": {
    "@next/env": "^13.2.1",
