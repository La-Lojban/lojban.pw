{
  "name": "lojban-made-easy",
  "version": "2.0.0",
  "scripts": {
    "dev": "NODE_ENV=development next",
    "build": "npx next telemetry disable && next build",
    "start": "next start",
    "print": "next start & sleep 5 && node lib/printer && ./lib/printer/shrink-pdf.sh",
    "export": "yarn build && next export",
    "deploy": "npx next telemetry disable && rm -rf node_modules/.cache && rimraf out && next build && yarn print && next export && touch out/.nojekyll && gh-pages --repo https://github.com/La-Lojban/lojban-made-easy.git -d out -t true && pkill node",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-brands-svg-icons": "^6.1.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^1.0.6",
    "@types/unist": "^2.0.0",
    "classnames": "2.3.1",
    "date-fns": "2.22.1",
    "github-slugger": "^1.3.0",
    "gray-matter": "4.0.3",
    "hast-util-parse-selector": "^3.1.0",
    "hast-util-select": "^5.0.2",
    "hastscript": "^6.0.0",
    "next": "12.2.2",
    "nprogress": "^0.2.0",
    "puppeteer": "13.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rehype-autolink-headings": "^5.1.0",
    "rehype-format": "^3.1.0",
    "rehype-raw": "^5.1.0",
    "rehype-stringify": "^8.0.0",
    "remark-deflist": "^0.3.0",
    "remark-extract-toc": "^1.1.0",
    "remark-gfm": "^1.0.0",
    "remark-html": "13.0.1",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.1.0",
    "sass": "^1.35.1",
    "tailwindcss-typography": "^3.1.0",
    "typescript": "^4.3.2",
    "unified": "^9.2.1",
    "unist-util-is": "^5.1.1",
    "unist-util-visit-parents": "^5.1.0"
  },
  "devDependencies": {
